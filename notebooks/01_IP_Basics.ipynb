    "# Generate sample IP addresses for analysis\n",
    "import random\n",
    "\n",
    "def generate_sample_ips(count=100):\n",
    "    \"\"\"Generate sample IP addresses across different classes\"\"\"\n",
    "    sample_ips = []\n",
    "    \n",
    "    for _ in range(count):\n",
    "        # Generate random IP based on class distribution\n",
    "        class_choice = random.choices(\n",
    "            ['A', 'B', 'C', 'D'], \n",
    "            weights=[20, 30, 40, 10]  # Weighted distribution\n",
    "        )[0]\n",
    "        \n",
    "        if class_choice == 'A':\n",
    "            first_octet = random.randint(1, 126)\n",
    "        elif class_choice == 'B':\n",
    "            first_octet = random.randint(128, 191)\n",
    "        elif class_choice == 'C':\n",
    "            first_octet = random.randint(192, 223)\n",
    "        else:  # Class D\n",
    "            first_octet = random.randint(224, 239)\n",
    "        \n",
    "        # Generate remaining octets\n",
    "        remaining_octets = [random.randint(0, 255) for _ in range(3)]\n",
    "        \n",
    "        ip = f\"{first_octet}.{'.'.join(map(str, remaining_octets))}\"\n",
    "        sample_ips.append(ip)\n",
    "    \n",
    "    return sample_ips\n",
    "\n",

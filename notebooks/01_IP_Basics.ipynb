    "## 1. IP Address Structure\n",
    "\n",
    "IPv4 addresses consist of 32 bits, typically written as four decimal numbers (octets) separated by periods."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Example IP addresses for analysis\n",
    "example_ips = [\n",
    "    \"192.168.1.1\",      # Class C Private\n",
    "    \"10.0.0.1\",         # Class A Private  \n",
    "    \"172.16.0.1\",       # Class B Private\n",
    "    \"8.8.8.8\",          # Class A Public (Google DNS)\n",
    "    \"127.0.0.1\",        # Loopback\n",
    "    \"224.0.0.1\",        # Multicast\n",
    "    \"169.254.1.1\"       # Link-local\n",
    "]\n",
    "\n",
    "# Analyze each IP address\n",
    "analysis_results = []\n",
    "\n",
    "for ip in example_ips:\n",
    "    info = classifier.identify_class(ip)\n",
    "    if info and 'error' not in info:\n",
    "        analysis_results.append({\n",
    "            'IP Address': ip,\n",
    "            'Class': info['class'],\n",
    "            'Type': 'Private' if info['is_private'] else 'Public',\n",
    "            'Special': info['special_range'] or 'Normal',\n",
    "            'Binary': info['binary_representation']['dotted_binary']\n",
    "        })\n",
    "\n",
    "# Create DataFrame for display\n",
    "df = pd.DataFrame(analysis_results)\n",
    "print(\"IP Address Analysis:\")\n",
    "print(df.to_string(index=False))"
   ]

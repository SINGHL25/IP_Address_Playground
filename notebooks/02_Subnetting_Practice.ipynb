    "def calculate_subnetting(base_network, required_subnets=None, required_hosts=None):\n",
    "    \"\"\"Calculate subnetting based on required subnets or hosts\"\"\"\n",
    "    try:\n",
    "        network = ipaddress.ip_network(base_network, strict=False)\n",
    "        base_prefix = network.prefixlen\n",
    "        \n",
    "        print(f\"\\nSubnetting Analysis for {base_network}\")\n",
    "        print(\"=\" * 50)\n",
    "        print(f\"Base Network: {network}\")\n",
    "        print(f\"Base Prefix: /{base_prefix}\")\n",
    "        print(f\"Available Host Bits: {32 - base_prefix}\")\n",
    "        \n",
    "        results = []\n",
    "        \n",
    "        if required_subnets:\n",
    "            # Calculate based on required number of subnets\n",
    "            subnet_bits = math.ceil(math.log2(required_subnets))\n",
    "            new_prefix = base_prefix + subnet_bits\n",
    "            \n",
    "            if new_prefix > 30:\n",
    "                print(f\"Error: Required subnets would result in /{new_prefix} which is not practical\")\n",
    "                return None\n",
    "            \n",
    "            actual_subnets = 2 ** subnet_bits\n",
    "            hosts_per_subnet = (2 ** (32 - new_prefix)) - 2\n",
    "            \n",
    "            print(f\"\\nBased on {required_subnets} required subnets:\")\n",
    "            print(f\"Subnet bits needed: {subnet_bits}\")\n",
    "            print(f\"New prefix length: /{new_prefix}\")\n",
    "            print(f\"Actual subnets available: {actual_subnets}\")\n",
    "            print(f\"Hosts per subnet: {hosts_per_subnet}\")\n",
    "            \n",
    "            # Generate subnet list\n",
    "            subnets = list(network.subnets(new_prefix=new_prefix))\n",
    "            \n",
    "            for i, subnet in enumerate(subnets[:min(10, len(subnets))]):  # Show first 10\n",
    "                first_host = list(subnet.hosts())[0] if list(subnet.hosts()) else \"N/A\"\n",
    "                last_host = list(subnet.hosts())[-1] if list(subnet.hosts()) else \"N/A\"\n",
    "                \n",
    "                results.append({\n",
    "                    'Subnet': i + 1,\n",
    "                    'Network': str(subnet),\n",
    "                    'First Host': str(first_host),\n",
    "                    'Last Host': str(last_host),\n",
    "                    'Broadcast': str(subnet.broadcast_address)\n",
    "                })\n",
    "        \n",
    "        elif required_hosts:\n",
    "            # Calculate based on required hosts per subnet\n",
    "            host_bits = math.ceil(math.log2(required_hosts + 2))  # +2 for network and broadcast\n",
    "            new_prefix = 32 - host_bits\n",
    "            \n",
    "            if new_prefix <= base_prefix:\n",
    "                print(f\"Error: Required hosts would need a prefix <= /{base_prefix}\")\n",
    "                return None\n",
    "            \n",
    "            actual_hosts = (2 ** host_bits) - 2\n",
    "            possible_subnets = 2 ** (new_prefix - base_prefix)\n",
    "            \n",
    "            print(f\"\\nBased on {required_hosts} required hosts per subnet:\")\n",
    "            print(f\"Host bits needed: {host_bits}\")\n",
    "            print(f\"New prefix length: /{new_prefix}\")\n",
    "            print(f\"Actual hosts per subnet: {actual_hosts}\")\n",
    "            print(f\"Possible subnets: {possible_subnets}\")\n",
    "            \n",
    "            # Generate subnet list\n",
    "            subnets = list(network.subnets(new_prefix=new_prefix))\n",
    "            \n",
    "            for i, subnet in enumerate(subnets[:min(10, len(subnets))]):  # Show first 10\n",
    "                first_host = list(subnet.hosts())[0] if list(subnet.hosts()) else \"N/A\"\n",
    "                last_host = list(subnet.hosts())[-1] if list(subnet.hosts()) else \"N/A\"\n",
    "                \n",
    "                results.append({\n",
    "                    'Subnet': i + 1,\n",
    "                    'Network': str(subnet),\n",
    "                    'First Host': str(first_host),\n",
    "                    'Last Host': str(last_host),\n",
    "                    'Broadcast': str(subnet.broadcast_address)\n",
    "                })\n",
    "        \n",
    "        if results:\n",
    "            print(f\"\\nFirst {min(10, len(results))} subnets:\")\n",
    "            df = pd.DataFrame(results)\n",
    "            print(df.to_string(index=False))\n",
    "            \n",
    "            if len(subnets) > 10:\n",
    "                print(f\"\\n... and {len(subnets) - 10} more subnets\")\n",
    "        \n",
    "        return results\n",
    "        \n",
    "    except Exception as e:\n",
    "        print(f\"Error in subnetting calculation: {e}\")\n",
    "        return None\n",
    "\n",
    "# Practice examples\n",
    "print(\"SUBNETTING EXAMPLES\")\n",
    "print(\"=\" * 30)\n",
    "\n",
    "# Example 1: Divide 192.168.1.0/24 into 4 subnets\n",
    "calculate_subnetting(\"192.168.1.0/24\", required_subnets=4)\n",
    "\n",
    "# Example 2: Design subnets for 50 hosts each\n",
    "calculate_subnetting(\"10.1.0.0/22\", required_hosts=50)"

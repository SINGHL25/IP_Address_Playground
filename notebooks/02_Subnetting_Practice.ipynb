    "        ])\n",
    "        print(f\"Binary mask: {formatted_binary}\")\n",
    "        \n",
    "        # Calculate values\n",
    "        total_addresses = 2 ** (32 - prefix_len)\n",
    "        usable_hosts = total_addresses - 2 if total_addresses > 2 else 0\n",
    "        \n",
    "        print(f\"Total addresses: 2^{32 - prefix_len} = {total_addresses}\")\n",
    "        print(f\"Usable hosts: {usable_hosts}\")\n",

    "        (\"range\", \"What is the host range for the 3rd subnet?\")\n",
    "    ]\n",
    "    \n",
    "    for i in range(5):\n",
    "        base_net = random.choice(base_networks)\n",
    "        prob_type, description = random.choice(problem_types)\n",

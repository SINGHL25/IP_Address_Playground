    "def design_vlsm_network(base_network, requirements):\n",
    "    \"\"\"\n",
    "    Design a VLSM network based on requirements\n",
    "    requirements: list of tuples (name, required_hosts)\n",
    "    \"\"\"\n",
    "    try:\n",
    "        network = ipaddress.ip_network(base_network, strict=False)\n",
    "        print(f\"\\nVLSM Design for {base_network}\")\n",
    "        print(\"=\" * 50)\n",
    "        \n",
    "        # Sort requirements by host count (largest first)\n",
    "        sorted_requirements = sorted(requirements, key=lambda x: x[1], reverse=True)\n",
    "        \n",
    "        print(\"Requirements (sorted by size):\")\n",
    "        for name, hosts in sorted_requirements:\n",
    "            print(f\"  {name}: {hosts} hosts\")\n",
    "        \n",
    "        vlsm_subnets = []\n",
    "        current_network = network\n",
    "        \n",
    "        for name, required_hosts in sorted_requirements:\n",
    "            # Calculate required host bits\n",
    "            host_bits = math.ceil(math.log2(required_hosts + 2))\n",
    "            prefix_length = 32 - host_bits\n",
    "            actual_hosts = (2 ** host_bits) - 2\n",
    "            \n",
    "            # Find a suitable subnet\n",
    "            try:\n",
    "                # Create subnet with calculated prefix length\n",
    "                subnet = ipaddress.ip_network(f\"{current_network.network_address}/{prefix_length}\", strict=False)\n",
    "                \n",
    "                # Ensure subnet fits within our available space\n",
    "                if subnet.supernet_of(current_network) or not current_network.supernet_of(subnet):\n",
    "                    # Find next available space\n",
    "                    available_networks = list(current_network.subnets(new_prefix=prefix_length))\n",
    "                    subnet = available_networks[0]\n",
    "                \n",
    "                first_host = list(subnet.hosts())[0] if list(subnet.hosts()) else \"N/A\"\n",
    "                last_host = list(subnet.hosts())[-1] if list(subnet.hosts()) else \"N/A\"\n",
    "                \n",
    "                vlsm_subnets.append({\n",
    "                    'Network Name': name,\n",
    "                    'Required Hosts': required_hosts,\n",
    "                    'Actual Hosts': actual_hosts,\n",
    "                    'Subnet': str(subnet),\n",
    "                    'First Host': str(first_host),\n",
    "                    'Last Host': str(last_host),\n",
    "                    'Broadcast': str(subnet.broadcast_address),\n",
    "                    'Efficiency': f\"{(required_hosts/actual_hosts)*100:.1f}%\"\n",
    "                })\n",
    "                \n",
    "                # Update current network for next allocation\n",
    "                # Find remaining space after this allocation\n",
    "                next_address = subnet.broadcast_address + 1\n",
    "                remaining_space = ipaddress.ip_network(\n",
    "                    f\"{next_address}/{current_network.prefixlen}\", strict=False\n",
    "                )\n",


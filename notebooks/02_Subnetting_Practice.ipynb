    "        problems.append({\n",
    "            'Problem': i + 1,\n",
    "            'Network': base_net,\n",
    "            'Question': description,\n",
    "            'Type': prob_type\n",
    "        })\n",
    "    \n",
    "    return problems\n",
    "\n",
    "def solve_practice_problem(problem_num, base_network, question_type):\n",
    "    \"\"\"Solve a practice problem\"\"\"\n",
    "    print(f\"\\nSOLUTION for Problem {problem_num}:\")\n",
    "    print(\"-\" * 30)\n",
    "    \n",
    "    try:\n",
    "        network = ipaddress.ip_network(base_network, strict=False)\n",
    "        \n",
    "        if \"26\" in question_type:\n",
    "            # Calculate /26 subnets\n",
    "            target_prefix = 26\n",
    "            if target_prefix <= network.prefixlen:\n",
    "                print(\"Cannot create /26 subnets from this network\")\n",
    "                return\n",
    "            \n",
    "            subnet_bits = target_prefix - network.prefixlen\n",
    "            num_subnets = 2 ** subnet_bits\n",
    "            hosts_per_subnet = (2 ** (32 - target_prefix)) - 2\n",
    "            \n",
    "            print(f\"Base network: {network}\")\n",
    "            print(f\"Target prefix: /{target_prefix}\")\n",
    "            print(f\"Number of /26 subnets: {num_subnets}\")\n",
    "            print(f\"Hosts per subnet: {hosts_per_subnet}\")\n",
    "            \n",
    "        elif \"30 hosts\" in question_type:\n",
    "            # Design for 30 hosts\n",
    "            required_hosts = 30\n",
    "            host_bits = math.ceil(math.log2(required_hosts + 2))\n",
    "            new_prefix = 32 - host_bits\n",
    "            actual_hosts = (2 ** host_bits) - 2\n",
    "            \n",
    "            if new_prefix <= network.prefixlen:\n",
    "                print(\"Network too small for required hosts\")\n",
    "                return\n",
    "            \n",
    "            possible_subnets = 2 ** (new_prefix - network.prefixlen)\n",
    "            \n",
    "            print(f\"Required hosts: {required_hosts}\")\n",
    "            print(f\"Optimal prefix: /{new_prefix}\")\n",
    "            print(f\"Actual hosts per subnet: {actual_hosts}\")\n",
    "            print(f\"Possible subnets: {possible_subnets}\")\n",
    "            \n",
    "        elif \"3rd subnet\" in question_type:\n",
    "            # Show 3rd subnet range\n",
    "            # Assume /26 for this example\n",
    "            target_prefix = min(26, network.prefixlen + 4)\n",
    "            subnets = list(network.subnets(new_prefix=target_prefix))\n",
    "            \n",
    "            if len(subnets) >= 3:\n",

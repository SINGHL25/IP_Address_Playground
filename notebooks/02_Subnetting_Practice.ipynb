   "source": [
    "def demonstrate_route_summarization():\n",
    "    \"\"\"Demonstrate route summarization concepts\"\"\"\n",
    "    print(\"ROUTE SUMMARIZATION EXAMPLE\")\n",
    "    print(\"=\" * 40)\n",
    "    \n",
    "    # Example networks to summarize\n",
    "    networks = [\n",
    "        \"192.168.0.0/24\",\n",
    "        \"192.168.1.0/24\", \n",
    "        \"192.168.2.0/24\",\n",
    "        \"192.168.3.0/24\"\n",
    "    ]\n",
    "    \n",
    "    print(\"Individual networks:\")\n",
    "    network_objects = []\n",
    "    for net in networks:\n",
    "        network_obj = ipaddress.ip_network(net)\n",
    "        network_objects.append(network_obj)\n",
    "        print(f\"  {net} ({network_obj.num_addresses} addresses)\")\n",
    "    \n",
    "    # Find summarization\n",
    "    try:\n",
    "        # Calculate the supernet that contains all networks\n",
    "        summarized = ipaddress.collapse_addresses(network_objects)\n",
    "        summary_networks = list(summarized)\n",
    "        \n",
    "        print(f\"\\nSummarized to:\")\n",
    "        total_addresses = 0\n",
    "        for summary in summary_networks:\n",
    "            print(f\"  {summary} ({summary.num_addresses} addresses)\")\n",
    "            total_addresses += summary.num_addresses\n",
    "        \n",
    "        original_routes = len(networks)\n",
    "        summarized_routes = len(summary_networks)\n",
    "        reduction = ((original_routes - summarized_routes) / original_routes) * 100\n",
    "        \n",
    "        print(f\"\\nRoute Reduction:\")\n",
    "        print(f\"  Original routes: {original_routes}\")\n",
    "        print(f\"  Summarized routes: {summarized_routes}\")\n",
    "        print(f\"  Reduction: {reduction:.1f}%\")\n",
    "        \n",
    "    except Exception as e:\n",
    "        print(f\"Error in summarization: {e}\")\n",
    "\n",
    "demonstrate_route_summarization()"

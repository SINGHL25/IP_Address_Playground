    "    # 3. Subnet mask calculations\n",
    "    print(\"\\n\\n3. SUBNET MASK CALCULATIONS\")\n",
    "    print(\"-\" * 40)\n",
    "    \n",
    "    def show_subnet_math(network_str):\n",
    "        network = ipaddress.ip_network(network_str)\n",
    "        prefix_len = network.prefixlen\n",
    "        \n",
    "        print(f\"\\nNetwork: {network}\")\n",
    "        print(f\"Prefix length: /{prefix_len}\")\n",
    "        print(f\"Network bits: {prefix_len}\")\n",
    "        print(f\"Host bits: {32 - prefix_len}\")\n",
    "        print(f\"Subnet mask: {network.netmask}\")\n",
    "        print(f\"Wildcard mask: {network.hostmask}\")\n",
    "        \n",
    "        # Show binary representation\n",
    "        mask_binary = '1' * prefix_len + '0' * (32 - prefix_len)\n",
    "        formatted_binary = '.'.join([\n",

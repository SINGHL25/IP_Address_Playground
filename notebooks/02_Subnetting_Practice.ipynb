    "def demonstrate_advanced_concepts():\n",
    "    \"\"\"Demonstrate advanced subnetting concepts\"\"\"\n",
    "    print(\"ADVANCED SUBNETTING CONCEPTS\")\n",
    "    print(\"=\" * 50)\n",
    "    \n",
    "    # 1. Supernetting example\n",
    "    print(\"\\n1. SUPERNETTING (Route Aggregation)\")\n",
    "    print(\"-\" * 40)\n",
    "    \n",
    "    individual_networks = [\n",
    "        \"10.1.0.0/24\",\n",
    "        \"10.1.1.0/24\",\n",
    "        \"10.1.2.0/24\",\n",
    "        \"10.1.3.0/24\"\n",
    "    ]\n",
    "    \n",
    "    print(\"Individual networks:\")\n",
    "    total_individual_hosts = 0\n",
    "    for net in individual_networks:\n",
    "        network = ipaddress.ip_network(net)\n",
    "        hosts = network.num_addresses - 2\n",
    "        total_individual_hosts += hosts\n",
    "        print(f\"  {net} - {hosts} hosts\")\n",
    "    \n",
    "    # Find supernet\n",
    "    supernet = ipaddress.ip_network(\"10.1.0.0/22\")\n",
    "    supernet_hosts = supernet.num_addresses - 2\n",
    "    \n",
    "    print(f\"\\nSupernet: {supernet} - {supernet_hosts} hosts\")\n",
    "    print(f\"Routing table reduction: 4 routes → 1 route (75% reduction)\")\n",
    "    print(f\"Address efficiency: {total_individual_hosts}/{supernet_hosts} = {(total_individual_hosts/supernet_hosts)*100:.1f}%\")\n",
    "    \n",
    "    # 2. CIDR and classless addressing\n",
    "    print(\"\\n\\n2. CLASSLESS vs CLASSFUL ADDRESSING\")\n",
    "    print(\"-\" * 40)\n",
    "    \n",
    "    example_ip = \"192.168.100.50\"\n",
    "    \n",
    "    print(f\"IP Address: {example_ip}\")\n",
    "    print(\"\\nClassful interpretation:\")\n",
    "    print(f\"  Class C network: 192.168.100.0/24\")\n",
    "    print(f\"  Available hosts: 254\")\n",
    "    \n",
    "    print(\"\\nClassless interpretations:\")\n",
    "    classless_examples = [\"/22\", \"/25\", \"/27\", \"/29\"]\n",
    "    for cidr in classless_examples:\n",
    "        network = ipaddress.ip_network(f\"{example_ip}{cidr}\", strict=False)\n",
    "        hosts = network.num_addresses - 2\n",
    "        print(f\"  {network} - {hosts} hosts\")\n",
